using System;
public class prime
{
    public static void Main(string[]args)
    {
        int i,n,m,flag=0;
        Console.Write("Enter the element: ");
        n=int.Parse(Console.ReadLine());
        m=n/2;
        for(i=2;i<=m;i++)
        {
            if(n%i==0)
            {
                Console.Write("The element is Not a prime.");
                flag=1;
            }
        }
        if(flag==0)
        {
            Console.Write("The element is Prime.");
        }
    }
}
